<?xml version="1.0" encoding="UTF-8"?>
<player>
	<audio id="player/audio"></audio>
	<div class="info">
		<div class="title"><span id="player/title"></span></div>
		<div class="artist"><span id="player/artist"></span></div>
		<div class="context">
			<div class="chip"><span id="player/chip"></span></div>
			<div class="software"><span id="player/software"></span></div>
		</div>
	</div>
	<div class="controls">
		<div class="play-pause" id="player/play-pause">
			<div data-lucide="play"></div>
			<div data-lucide="pause"></div>
		</div>
		<div>
			<div id="player/prev">
				<div data-lucide="skip-back"></div>
			</div>
			<div id="player/next">
				<div data-lucide="skip-forward"></div>
			</div>
		</div>
		<a id="player/reveal">
			<div data-lucide="list-music"></div>
		</a>
	</div>
	<div class="duration">
		<div class="left">
			<div id="player/elapsed">elapsed</div>
		</div>
		<div class="right">
			<div id="player/remaining">remaining</div>
			/
			<div id="player/total">total</div>
		</div>
	</div>
	<div class="progress">
		<div id="player/progress"></div>
	</div>
</player>